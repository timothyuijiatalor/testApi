{
  "name": "new-typeorm-project",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^8.10.66",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.38"
  },
  "scripts": {
    "clean": "npx rimraf build",
    "build": "npm run clean && npx tsc --p tsconfig.json && npx ts-node scripts/build.ts",
    "lint": "npx eslint --fix \"{src,scripts}/**/*.{ts,js}\"",
    "format": "npx prettier --write \"{src,scripts}/**/*.{ts,js}\"",
    "start": "node build/index.js",
    "start:dev": "npx ts-node-dev --prefer-ts true --no-notify --watch src --respawn src/index.ts",
    "prepare": "npx husky install"
  }
}
